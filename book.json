  "pluginsConfig": {
    "sitemap-general": {
      "prefix": "https://circlereader.com/"
    },
    "rss": {
      "title": "Circle 阅读助手",
      "description": "一款提供沉浸式阅读的扩展，拥有强大智能的识别和排版能力，让你爱上在网页上阅读",
      "author": "染河<wenguang.fe@gmail.com>",
      "feed_url": "https://circlereader.com/docs/rss",
      "site_url": "https://circlereader.com/",
      "managingEditor": "wenguang.fe@gmail.com",
      "webMaster": "wenguang.fe@gmail.com"
    },
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "sidebar-style": {
      "title": "Circle 阅读助手",
      "author": "染河"
    },
    "insert-logo": {
      "url": "assets/logo.jpg",
      "style": "width:60px;height:auto;"
