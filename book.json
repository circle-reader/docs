  "generator": "site",
  "author": "染河<wenguang.fe@gmail.com>",
  "links": {
    "sidebar": {
      "<< 返回官网": "https://circlereader.com/"
    }
  },
  "pdf": {
    "fontSize": 12,
    "footerTemplate": null,
    "headerTemplate": null,
    "margin": {
      "bottom": 36,
      "left": 36,
      "right": 36,
      "top": 36
    },
    "pageNumbers": true,
    "paperSize": "a4"
